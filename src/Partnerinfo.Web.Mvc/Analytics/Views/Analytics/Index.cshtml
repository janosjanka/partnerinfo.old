@{
    ViewBag.AppTitle.Enqueue("Analitikák");
    ViewBag.AppClass.Enqueue("ui-theme-wide");
    ViewBag.AppClass.Enqueue("ui-theme-gray");
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="ui-main-header">
    <div class="ui-container">
        <div role="group" class="ui-btn-group">
            <button class="ui-btn ui-btn-flat" type="button" data-bind='click: function () { $(".ui-sidebar").toggleClass("ui-collapse"); }, clickBubble: false'>
                <i class="i menu"></i>
            </button>
        </div>
    </div>
</div>
<div class="ui-main-content">
    <div class="ui-sidebar ui-collapse">
        <div class="ui-container" id="piEventsMenu"></div>
    </div>
    <div class="ui-canvas">
        <div data-bind='ui: { name: "logging.events", categories: { element: "#piEventsMenu" } }'></div>
    </div>
</div>